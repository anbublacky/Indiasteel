<% @companyprofiles.each do |companyprofile| %>
  <tr>
    <td><%= companyprofile.Company_name %></td>
    <td><%= companyprofile.Owner_name %></td>
    <td><%= companyprofile.Address %></td>
    <td><%= companyprofile.Company_status %></td>
    <td><%= companyprofile.Date_of_Establishment %></td>
    <td><%= companyprofile.Factory_space_status %></td>
    <td><%= companyprofile.Telephone_No %></td>
    <td><%= companyprofile.Fax_No %></td>
    <td><%= companyprofile.E_Mail %></td>
    <td><%= companyprofile.Contact_Person %></td>
    <td><%= companyprofile.Cell_no %></td>
    <td><%= companyprofile.Nature_of_Business %></td>
    <td><%= companyprofile.Local_sales_tax %></td>
    <td><%= companyprofile.Central_sales_tax %></td>
    <td><%= companyprofile.TIN_No %></td>
    <td><%= companyprofile.Central_excise %></td>
    <td><%= companyprofile.Income_tax_PA_No %></td>
    <td><%= companyprofile.No_of_Employee %></td>
    <td><%= companyprofile.Branches_Address %></td>
    <td><%= companyprofile.Turnover_past3year %></td>
    <td><%= companyprofile.BankersandAddress %></td>
    <td><%= companyprofile.Bank_Account_No %></td>
    <td><%= companyprofile.Our_Manufacturer %></td>
    <td><%= companyprofile.Our_Major_Customer %></td>
    <td><%= link_to 'Show', companyprofile %></td>
    <td><%= link_to 'Edit', edit_companyprofile_path(companyprofile) %></td>
    <td><%= link_to 'Destroy', companyprofile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

