<table>
  <tr>
    <th>Company name</th>
    <th>Owner name</th>
    <th>Address</th>
    <th>Company status</th>
    <th>Date of establishment</th>
    <th>Factory space status</th>
    <th>Telephone no</th>
    <th>Fax no</th>
